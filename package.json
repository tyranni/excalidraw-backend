    },
    "devDependencies": {
        "@jitsi/eslint-config": "^4.1.12",
        "@typescript-eslint/eslint-plugin": "6.12.0",
        "@typescript-eslint/parser": "6.12.0",
        "eslint": "8.54.0",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-jsdoc": "46.9.0",
        "eslint-plugin-typescript-sort-keys": "^3.1.0"
